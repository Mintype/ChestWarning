{
  "schemaVersion": 1,
  "id": "chestwarningmod",
  "version": "${version}",
  "name": "ChestWarningMod",
  "description": "A Minecraft Fabric mod that warns players when opening chests they might not own, preventing accidental theft.",
  "authors": ["Mintype"],
  "contact": {},
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "org.mintype.chestwarningmod.client.ChestwarningmodDataGenerator"
    ],
    "client": [
      "org.mintype.chestwarningmod.client.ChestwarningmodClient"
    ],
    "main": [
      "org.mintype.chestwarningmod.Chestwarningmod"
    ]
  },
  "mixins": [
    "chestwarningmod.mixins.json",
    {
      "config": "chestwarningmod.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  }
}
